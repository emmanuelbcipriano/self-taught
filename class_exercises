class Rectangle():
    
    def __init__ (self, w, l):
        self.width = w
        self.length = l
        
    def calculate_perimeter (self):
        print(2*self.width+2*self.length)    

class Square():
    def __init__ (self, s):
        self.s1 = s
        
    def calculate_perimeter (self):
        print(4*self.s1)
        
rec = Rectangle (2,3)
sq = Square (4)

rec.calculate_perimeter()
sq.calculate_perimeter()